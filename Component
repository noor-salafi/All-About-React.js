About Component

-- (Functional Component) --

import React from "react";

function Home() {
return(
 <>
    <h1>Home</h1>
 </>
)
};
export default Home;

shortCut for create functional Component;
rfce;


-- (Class Component) --

import React, { Component } from 'react'

 class Home extends Component {
  render() {
    return (
      <>
        <h1>Home</h1>
      </>
    )
  }
};
export default Home;
